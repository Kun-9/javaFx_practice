<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      alignment="CENTER" prefHeight="300.0" prefWidth="400.0" spacing="10.0" >

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

    <children>
        <BarChart fx:id="barChart" prefHeight="250.0" prefWidth="Infinity" style="-fx-border-color: black;">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>

            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>

        </BarChart>

        <Button fx:id="cancelBtn" alignment="CENTER" mnemonicParsing="false"
                prefHeight="20.0" prefWidth="50.0" text="닫기" textAlignment="CENTER" />
    </children>
</VBox>


